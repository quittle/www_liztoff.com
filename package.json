{
    "name": "spark-l",
    "version": "1.0.0",
    "description": "Liz Toff's personal website",
    "main": "index.tsx",
    "scripts": {
        "build": "webpack --mode production",
        "start": "webpack-dev-server",
        "test": "npm run check",
        "prettier": "prettier --check '**'",
        "prettier-fix": "prettier --write '**'",
        "eslint": "eslint --ext '.ts,.tsx' src",
        "eslint-fix": "eslint --fix --ext '.ts,.tsx' src",
        "stylelint": "stylelint src/**/*.scss",
        "stylelint-fix": "stylelint src/**/*.scss --fix",
        "check": "npm run prettier && npm run eslint && npm run stylelint",
        "release": "npm run build && npm run test"
    },
    "author": "Dustin Toff",
    "license": "Apache-2.0",
    "devDependencies": {
        "@types/react": "^16.9.55",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^4.6.0",
        "@typescript-eslint/parser": "^4.6.0",
        "css-loader": "^4.3.0",
        "css-webpack-plugin": "^1.0.2",
        "eslint": "^7.12.1",
        "eslint-plugin-react": "^7.21.5",
        "file-loader": "^6.2.0",
        "node-sass": "^4.14.1",
        "prettier": "^2.1.2",
        "sass-loader": "^10.0.4",
        "source-map-loader": "^1.1.2",
        "style-loader": "^2.0.0",
        "stylelint": "^13.7.2",
        "stylelint-config-standard": "^20.0.0",
        "ts-loader": "^8.0.7",
        "typescript": "^4.0.3",
        "webpack": "^4.44.2",
        "webpack-cli": "^4.1.0",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "framer-motion": "^2.9.4",
        "html-webpack-plugin": "^4.5.0",
        "react": "^16.14.0",
        "react-anime": "^3.0.3",
        "react-dom": "^16.13.1"
    }
}
